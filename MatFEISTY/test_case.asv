%%%%!! RUN HISTORIC FOR ALL LOCATIONS
function test_case()

%%%%%%%%%%%%%%% Initialize Model Variables
%! Make core parameters/constants
param = make_parameters_1meso();

%! Idealized bathymetry
X = [  30.04108171,   30.19902725,   31.17873201,   36.8321355 ,...
    61.34734988,  131.85502453,  264.42098685,  437.55383604,...
    609.22301965,  749.85055796,  851.52743149,  919.48569869,...
    962.76570279,  989.53462509, 1005.8032774 , 1015.58739719,...
    1021.43508524, 1024.91716307, 1026.98605941, 1028.21370613,...
    1028.94160815, 1029.37300191];

param.NX = length(X);
param.ID = 1:param.NX;
NX = param.NX;
ID = 1:param.NX;

%! Idealized forcing
load('feisty.testcase.forcing_cyclic.mat')

%! How long to run the model
YEARS = 1;
DAYS = 365;

%! Create a directory for output
exper = 'v14_';
[fname,simname] = sub_fname_cesm_fosi_exper(param,exper);

%! Initialize
[Sml_f,Sml_p,Sml_d,Med_f,Med_p,Med_d,Lrg_p,Lrg_d,BENT] = sub_init_fish_spin(ID,DAYS);

%! Storage variables
biomass = NaN*ones(DAYS,9,NX);
T_habitat = NaN*ones(DAYS,9,NX);
ingestion_rate = NaN*ones(DAYS,9,NX);
predation_flux = NaN*ones(DAYS,9,NX);
predation_rate = NaN*ones(DAYS,9,NX);
metabolism_rate = NaN*ones(DAYS,9,NX);
mortality_rate = NaN*ones(DAYS,9,NX);
energy_avail_rate = NaN*ones(DAYS,9,NX);
growth_rate = NaN*ones(DAYS,9,NX);
reproduction_rate = NaN*ones(DAYS,9,NX);
recruitment_flux = NaN*ones(DAYS,9,NX);
fish_catch_rate = NaN*ones(DAYS,9,NX);

%% %%%%%%%%%%%%%%%%%%%% Run the Model
MNT = 0;
%! Run model with no fishing
for YR = 1:YEARS % years
    for DAY = 1:param.DT:DAYS % days
        
        %%%! Future time step
        DY = int64(ceil(DAY));
        [num2str(YR),' , ', num2str(mod(DY,365))]
        
        [Sml_f,Sml_p,Sml_d,Med_f,Med_p,Med_d,Lrg_p,Lrg_d,BENT,ENVR] = ...
            sub_futbio_1meso(DY,ESM,GRD,Sml_f,Sml_p,Sml_d,...
            Med_f,Med_p,Med_d,Lrg_p,Lrg_d,BENT,param);
        
        %! Store
        Spinup_Sml_f(DY,:,:) = [Sml_f.nu Sml_f.gamma Sml_f.die Sml_f.rep ...
            Sml_f.rec Sml_f.clev Sml_f.prod Sml_f.pred Sml_f.nmort Sml_f.met ...
            Sml_f.caught Sml_f.fmort]';
        
        biomass(DY,:,:) = [Sml_f.bio; Sml_p.bio; Sml_d.bio;...
                          Med_f.bio; Med_p.bio; Med_d.bio;...
                          Lrg_p.bio; Lrg_d.bio; BENT.mass];
        T_habitat = %avg habitat temp -> need to calc
        ingestion_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        predation_flux(DY,1:8,:) = [Sml_f.die; Sml_p.die; Sml_d.die;...
                                   Med_f.die; Med_p.die; Med_d.die;...
                                   Lrg_p.die; Lrg_d.die];
        predation_rate(DY,1:8,:) = [Sml_f.pred; Sml_p.pred; Sml_d.pred;...
                                   Med_f.pred; Med_p.pred; Med_d.pred;...
                                   Lrg_p.pred; Lrg_d.pred];
        metabolism_rate(DY,1:8,:) = [Sml_f.met; Sml_p.met; Sml_d.met;...
                                   Med_f.met; Med_p.met; Med_d.met;...
                                   Lrg_p.met; Lrg_d.met];
        mortality_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        energy_avail_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        growth_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        reproduction_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        recruitment_flux(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        fish_catch_rate(DY,1:8,:) = [Sml_f.I; Sml_p.I; Sml_d.I;...
                                   Med_f.I; Med_p.I; Med_d.I;...
                                   Lrg_p.I; Lrg_d.I];
        
    end %Days
    
end %Years

%%% Save
save([fname '_locs.mat'],...
    'S_Sml_f','S_Sml_p','S_Sml_d','S_Med_f','S_Med_p','S_Med_d',...
    'S_Lrg_p','S_Lrg_d','S_LTL')

end